            <a class="active" href="@Url.Action("Index","Home")">Home</a>

            <a runat="server" href="~/RDLC_ReportTutorial.aspx">AdattipusokRDLC</a>
            <a runat="server" href="~/Adattipus.aspx">AdattipusokRDLC</a>
            <a runat="server" href="~/Default.aspx">TanarTablazat</a>


            <a class="last" href="@Url.Action("LogOut","Login")">Log Out</a>
            <a class="last" href="@Url.Action("ToProfil","Home")">Profile</a>




        </div>
    </div>
